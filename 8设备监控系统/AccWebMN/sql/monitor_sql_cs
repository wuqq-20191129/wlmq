create table MTR_COMMAND_MODULE_MAPPING
(
  command    VARCHAR2(100) not null,
  menu_id    VARCHAR2(6) not null,
  table_name VARCHAR2(100) not null
)
tablespace ACC_MONITOR
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
