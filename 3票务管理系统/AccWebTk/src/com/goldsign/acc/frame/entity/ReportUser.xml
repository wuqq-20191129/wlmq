<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goldsign.acc.frame.mapper.ReportUserMapper">
    
   
    <select id="getReportUser"  parameterType="String" resultType="com.goldsign.acc.frame.entity.ReportUser">
        select ds_id,ds_user,ds_pass from w_acc_st.w_op_cfg_report_pass 
                                     where ds_id=#{id}
 
    </select>
   
     
    
</mapper>
