create table MTR_MENU
(
  menu_id         VARCHAR2(6) not null,
  menu_name       VARCHAR2(50),
  menu_url        VARCHAR2(100),
  menu_icon       VARCHAR2(50),
  top_menu_id     VARCHAR2(2),
  parent_id       VARCHAR2(4),
  locked          CHAR(1),
  post_parameter  VARCHAR2(250),
  status          VARCHAR2(1) default '0' not null,
  new_window_flag VARCHAR2(1) default '0' not null,
  password_flag   VARCHAR2(1) default '0' not null
)
tablespace ACC_MONITOR
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
